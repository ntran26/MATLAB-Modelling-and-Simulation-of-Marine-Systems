% Simulation of the open-loop with trajectory (turning circle)

clear, clc, close all;

function [xdot, Y] = M6_A1(x, u, t)
    T = 7.5;
    K = 0.11;
    ui = 12*1850/3600;   % surge velocity [m/s]
    vi = 0*1850/3600;    % sway velocity [m/s]
    
    % State equation
    xdot = [x(2)
            -1/T*x(2) + K/T*u(1)
            ui*cos(x(1)) + vi*sin(x(1))
            ui*sin(x(1)) - vi*cos()];

    % Output equation
    Y = [1 0;0 1]*[x(1);x(2)] + [0;0]*[u(1)];

end
